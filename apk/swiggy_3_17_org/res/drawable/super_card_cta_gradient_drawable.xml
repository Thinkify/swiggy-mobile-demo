<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/super_recommended_end_color" android:endColor="@color/super_recommended_start_color" android:angle="90.0" android:type="linear" />
            <stroke android:width="@dimen/button_border" android:color="@color/orange30" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/super_recommended_end_color" android:endColor="@color/super_recommended_start_color" android:angle="90.0" android:type="linear" />
        </shape>
    </item>
</selector>