<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_activated="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white100" />
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/blackGrape100_10" />
        </shape>
    </item>
    <item android:state_activated="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/dark_peach" />
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/dark_peach_border" />
        </shape>
    </item>
</selector>