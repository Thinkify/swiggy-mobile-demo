<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle"
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/white100" />
            <stroke android:width="1.0dip" android:color="@color/veg_indicator" />
            <size android:height="@dimen/toggle_fg_size" android:width="@dimen/toggle_fg_size" />
        </shape>
    </item>
    <item android:left="@dimen/toggle_fg_circle_padding" android:top="@dimen/toggle_fg_circle_padding" android:right="@dimen/toggle_fg_circle_padding" android:bottom="@dimen/toggle_fg_circle_padding">
        <shape android:shape="oval"
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/veg_indicator" />
        </shape>
    </item>
</layer-list>